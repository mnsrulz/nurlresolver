{
  "name": "nurlresolver",
  "version": "3.4.0",
  "description": "url resolver for node",
  "type": "module",
  "exports": "./dist/app.js",
  "types": "./dist/app.d.ts",
  "engines": {
    "node": ">=14.16"
  },
  "author": "mnsrulz",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.4",
    "got": "^12.6.0",
    "m3u8": "0.0.10",
    "psl": "^1.9.0",
    "scrape-it": "^6.0.1",
    "tough-cookie": "^4.1.2",
    "unpacker": "^1.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/psl": "^1.1.0",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "ava": "^5.2.0",
    "eslint": "^8.35.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "ava",
    "build": "tsc",
    "build:watch": "tsc -w",
    "prepublishOnly": "tsc",
    "start:debug": "nodemon -I --exec DEBUG=nurl* node --experimental-specifier-resolution=node --loader ts-node/esm src/app.debug.ts"
  },
  "ava": {
    "files": [
      "tests/*"
    ],
    "timeout": "1m",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnsrulz/nurlresolver.git"
  },
  "bugs": {
    "url": "https://github.com/mnsrulz/nurlresolver/issues"
  },
  "homepage": "https://github.com/mnsrulz/nurlresolver#readme",
  "keywords": [
    "urlresolver",
    "megaup",
    "googledrive",
    "link extractor",
    "scraper",
    "download link",
    "cloudmail",
    "link generator",
    "direct link"
  ]
}
